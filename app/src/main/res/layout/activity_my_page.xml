<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/graybackground"
    tools:context=".MyPageActivity">

    <ImageView
        android:id="@+id/mypageImage1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/mypage1" />

    <ImageView
        android:id="@+id/myPageImage2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="@id/mypageImage1"
        app:layout_constraintRight_toRightOf="@id/mypageImage1"
        app:layout_constraintTop_toBottomOf="@id/mypageImage1"
        app:srcCompat="@drawable/mypage2" />

    <ImageView
        android:id="@+id/myPageImage4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        app:layout_constraintStart_toStartOf="@id/myPageImage2"
        app:layout_constraintTop_toTopOf="@id/myPageImage2"
        app:srcCompat="@drawable/mypage4" />

    <TextView
        android:id="@+id/myPageUserText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginRight="100dp"
        android:text="User"
        android:textColor="#000000"
        android:textStyle="bold"
        app:layout_constraintRight_toRightOf="@id/myPageImage2"
        app:layout_constraintTop_toTopOf="@id/myPageImage2" />

    <ImageView
        android:id="@+id/nameBlank"
        android:layout_width="150dp"
        android:layout_height="50dp"
        app:layout_constraintRight_toRightOf="@id/myPageImage2"
        app:layout_constraintTop_toBottomOf="@id/myPageUserText"
        app:srcCompat="@drawable/blankbar" />

    <TextView
        android:id="@+id/nametv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="sans-serif-black"
        android:text="이름"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/nameBlank"
        app:layout_constraintTop_toTopOf="@id/nameBlank" />

    <EditText
        android:id="@+id/nameEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:background="@null"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/nameBlank"
        app:layout_constraintTop_toTopOf="@id/nameBlank" />

    <ImageView
        android:id="@+id/phoneBlank"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginTop="30dp"
        app:layout_constraintRight_toRightOf="@id/myPageImage2"
        app:layout_constraintTop_toBottomOf="@id/myPageUserText"
        app:srcCompat="@drawable/blankbar" />

    <TextView
        android:id="@+id/phoneNumtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:fontFamily="sans-serif-black"
        android:text="전화번호"
        android:textColor="#000000"
        android:textSize="10sp"
        app:layout_constraintEnd_toStartOf="@+id/phoneBlank"
        app:layout_constraintTop_toTopOf="@id/phoneBlank" />

    <EditText
        android:id="@+id/phoneEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="15dp"
        android:background="@null"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/phoneBlank"
        app:layout_constraintTop_toTopOf="@id/phoneBlank" />

    <ImageView
        android:id="@+id/myPageImage3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@id/myPageImage2"
        app:layout_constraintTop_toBottomOf="@id/myPageImage2"
        app:srcCompat="@drawable/mypage3" />

    <TextView
        android:id="@+id/myPageStoreText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="23dp"
        android:text="Store"
        android:textColor="#000000"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="@id/myPageImage3"
        app:layout_constraintTop_toTopOf="@id/myPageImage3" />

    <ImageView
        android:id="@+id/myPageImage5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@id/myPageImage3"
        app:layout_constraintRight_toRightOf="@id/myPageImage3"
        app:srcCompat="@drawable/mypage5" />

    <ImageView
        android:id="@+id/storeNameBlank"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintLeft_toLeftOf="@id/myPageImage3"
        app:layout_constraintTop_toBottomOf="@id/myPageStoreText"
        app:srcCompat="@drawable/blankbar" />

    <TextView
        android:id="@+id/storeNametv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:text="매장명"
        android:textColor="#000000"
        android:textSize="10sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/storeNameBlank"
        app:layout_constraintTop_toTopOf="@id/storeNameBlank" />

    <EditText
        android:id="@+id/mpStoreNameEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/storeNameBlank"
        app:layout_constraintTop_toTopOf="@id/storeNameBlank" />


    <ImageView
        android:id="@+id/storeNumBlank"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="30dp"
        app:layout_constraintLeft_toLeftOf="@id/myPageImage3"
        app:layout_constraintTop_toBottomOf="@id/myPageStoreText"
        app:srcCompat="@drawable/blankbar" />

    <EditText
        android:id="@+id/mpStoreNumEdit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:background="@null"
        android:ems="8"
        android:inputType="textPersonName"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="@id/storeNumBlank"
        app:layout_constraintTop_toTopOf="@id/storeNumBlank" />

    <TextView
        android:id="@+id/storeNumtv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="16dp"
        android:fontFamily="sans-serif-black"
        android:text="매장번호"
        android:textColor="#000000"
        android:textSize="10sp"

        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@id/storeNumBlank"

        app:layout_constraintTop_toTopOf="@id/storeNumBlank" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:layout_marginLeft="30sp"
        android:layout_marginRight="30sp"
        android:orientation="horizontal"
        app:layout_constraintTop_toBottomOf="@id/myPageImage3">

        <ImageButton
            android:id="@+id/modify_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="23.6dp"
            android:layout_weight="1"
            android:background="@color/graybackground"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/logout_btn"
            app:layout_constraintTop_toBottomOf="@id/myPageImage3"
            app:srcCompat="@drawable/modify" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="center"
            android:layout_marginTop="23.6dp"
            app:layout_constraintLeft_toLeftOf="@id/modify_btn"
            app:layout_constraintRight_toRightOf="@id/logout_btn"
            app:layout_constraintTop_toBottomOf="@id/myPageImage3">

            <ImageView
                android:id="@+id/kakao_image"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/graybackground"
                app:layout_constraintLeft_toRightOf="@id/logout_btn"
                app:layout_constraintRight_toRightOf="@id/kakao_btn"
                app:layout_constraintTop_toBottomOf="@id/myPageImage3"
                app:srcCompat="@drawable/kakao" />

            <ImageButton
                android:id="@+id/kakao_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@color/graybackground"
                android:onClick="kakaoLink"
                app:layout_constraintLeft_toLeftOf="@id/kakao_image"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/myPageImage3"
                app:srcCompat="@drawable/invite" />
        </LinearLayout>
        <ImageButton
            android:id="@+id/logout_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="23.6dp"
            android:layout_weight="1"
            android:background="@color/graybackground"
            app:layout_constraintLeft_toRightOf="@id/modify_btn"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/myPageImage3"
            app:srcCompat="@drawable/logout" />
    </LinearLayout>


        <TextView
            android:id="@+id/postv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text=""
            android:textColor="#f3e7d5"
            android:textSize="12sp"
            app:layout_constraintLeft_toRightOf="@id/myPageStoreText"
            app:layout_constraintTop_toTopOf="@id/myPageStoreText" />

</androidx.constraintlayout.widget.ConstraintLayout>